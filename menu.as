package  {		import flash.display.MovieClip;			public class menu extends MovieClip {		var menuscreen;				public function menu(xPos, yPos, rotate) {			// constructor code						menuscreen = new bg_brown();			this.addChild(menuscreen);						trace('new menu created');									this.x = xPos;			this.y = yPos;						this.width = 480;			this.height = 300;						if (rotate == true){				menuscreen.rotation = -180;			};		}	}	}