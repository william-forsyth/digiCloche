package {	import flash.events.Event;	import org.papervision3d.objects.parsers.DAE;		[SWF(width=640, height=480, backgroundColor=0x808080, frameRate=30)]		public class Earth extends PV3DARApp {				private var _earth:DAE;				public function Earth() {			addEventListener(Event.INIT, _onInit);			init('Data/camera_para.dat', 'Data/flarlogo.pat');			//init('Data/camera_para.dat', 'Data/marker1.pat');		}				private function _onInit(e:Event):void {			_earth = new DAE();			//_earth.load('model/Pokeball.dae');			_earth.load('model/bread.dae');			_earth.scale = 1;			_earth.rotationX = 0;			_earth.rotationY = 45;			_markerNode.addChild(_earth);						addEventListener(Event.ENTER_FRAME, _update);		}				private function _update(e:Event):void {			_earth.rotationZ -= 0.5		}	}}