package {	import flash.events.Event;	import org.papervision3d.objects.parsers.DAE;		[SWF(width=640, height=480, backgroundColor=0x808080, frameRate=30)]		public class Earth2 extends PV3DARApp {				private var _earth:DAE;				public function Earth2() {			addEventListener(Event.INIT, _onInit);			init('Data/camera_para.dat', 'Data/flarlogo.pat');		}				private function _onInit(e:Event):void {			_earth = new DAE();			_earth.load('model/Candycane.dae');			_earth.scale = 50;			_earth.rotationX = 0;			_earth.rotationY = 0;			_markerNode.addChild(_earth);						addEventListener(Event.ENTER_FRAME, _update);		}				private function _update(e:Event):void {			_earth.rotationZ -= 0.5		}	}}