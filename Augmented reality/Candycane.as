package {	import flash.events.Event;	import org.papervision3d.objects.parsers.DAE;		[SWF(width=640, height=480, backgroundColor=0x808080, frameRate=30)]		public class Candycane extends PV3DARApp {				private var _Candycane:DAE;				public function Candycane() {			addEventListener(Event.INIT, _onInit);			init('Data/camera_para.dat', 'Data/flarlogo.pat');		}				private function _onInit(e:Event):void {			_Candycane = new DAE();			_Candycane.load('model/Candycane.dae');			_Candycane.scale = 50;			_Candycane.rotationX = 0;			_Candycane.rotationY = 0;			_markerNode.addChild(_Candycane);						addEventListener(Event.ENTER_FRAME, _update);		}				private function _update(e:Event):void {			_Candycane.rotationZ -= 0.5		}	}}