height			var roundRect:Shape = new Shape();			roundRect.graphics.beginFill(0xFFFFFF, 0.5);			roundRect.graphics.drawRoundRect(0, 0, 600, 280, 15, 15);			roundRect.x = -300;			roundRect.y = -100;			this.addChild(roundRect);			champagne = new alco5();			champagne.x = align_left;			champagne.y = align_top;									this.addChild(champagne);						beer = new alco2();			beer.x = align_left/2;			beer.y = align_top;			///beer.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{ showInfo(e, "beer.txt")});			this.addChild(beer);			whiskey = new alco4();			whiskey.y = align_top;			//whiskey.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{ showInfo(e, "whiskey.txt")});			this.addChild(whiskey);			sake = new alco3();			sake.x = align_right / 2;			sake.y = align_top;			//sake.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{ showInfo(e, "sake.txt")});			this.addChild(sake);			circles = [beer, whiskey, champagne, sake];			for each (var i in circles)			{				//i.addEventListener(MouseEvent.MOUSE_DOWN, startDragging);				i.addEventListener(MouseEvent.MOUSE_UP, stopDragging);				i.startX = i.x;				i.startY = i.y;				MultiTouch.enableGesture(i,new MultiDragScaleRotate(true,true,true,true,null,110,180));			}						backbutton = new back();			backbutton.x = -270;			backbutton.y = -170;			this.addChild(backbutton);									/*LABELS*/			champagne_label = new CheckBox();			champagne_label.label = "Champagne (bottle)";			champagne_label.width = 300;			champagne_label.x = align_left - 40;			champagne_label.y = champagne.y + 40;			champagne_label.setStyle("embedFonts", true);			champagne_label.setStyle("textFormat", myTextFormat);			this.addChild(champagne_label);						beer_label = new CheckBox();			beer_label.label = "XXXX Gold";			beer_label.width = 300;			beer_label.x = align_left/2 - 40;			beer_label.y = beer.y + 40;			beer_label.setStyle("embedFonts", true);			beer_label.setStyle("textFormat", myTextFormat);			this.addChild(beer_label);						whiskey_label = new CheckBox();			whiskey_label.label = "Whiskey (200mL bottle)";			whiskey_label.width = 300;			whiskey_label.x = - 40;			whiskey_label.y = whiskey.y + 40;			whiskey_label.setStyle("embedFonts", true);			whiskey_label.setStyle("textFormat", myTextFormat);			this.addChild(whiskey_label);						sake_label = new CheckBox();			sake_label.label = "Sake";			sake_label.width = 300;			sake_label.x = align_right/2 - 40;			sake_label.y = sake.y + 40;			sake_label.setStyle("embedFonts", true);			sake_label.setStyle("textFormat", myTextFormat);			this.addChild(sake_label);						/*View info lists Created*/						viewInfo1.defaultTextFormat = linkFormat;			viewInfo1.embedFonts = true;			viewInfo1.width = 300;			viewInfo1.x = champagne.x - 20;			viewInfo1.y = champagne.y + 55;			viewInfo1.wordWrap = true;			viewInfo1.text = "View Info" ;			viewInfo1.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{showInfo(e, "champagne.txt")});			this.addChild(viewInfo1);						viewInfo2.defaultTextFormat = linkFormat;			viewInfo2.embedFonts = true;			viewInfo2.width = 300;			viewInfo2.x = beer.x - 20;			viewInfo2.y = beer.y + 55;			viewInfo2.wordWrap = true;			viewInfo2.text = "View Info" ;			viewInfo2.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{showInfo(e, "beer.txt")});			this.addChild(viewInfo2);							viewInfo3.defaultTextFormat = linkFormat;			viewInfo3.embedFonts = true;			viewInfo3.width = 300;			viewInfo3.x = whiskey.x - 20;			viewInfo3.y = whiskey.y + 55;			viewInfo3.wordWrap = true;			viewInfo3.text = "View Info" ;			viewInfo3.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{showInfo(e, "whiskey.txt")});			this.addChild(viewInfo3);						viewInfo4.defaultTextFormat = linkFormat;			viewInfo4.embedFonts = true;			viewInfo4.width = 300;			viewInfo4.x = sake.x - 20;			viewInfo4.y = sake.y + 55;			viewInfo4.wordWrap = true;			viewInfo4.text = "View Info" ;			viewInfo4.addEventListener(MouseEvent.MOUSE_DOWN, function(e:MouseEvent):void{showInfo(e, "sake.txt")});			this.addChild(viewInfo4);						arimg = new patt4();			arimg.y = 130;			this.addChild(arimg);					}		function startDragging(event:MouseEvent):void		{//, obj:Object) {			//obj.startDrag();			// no longer need to keep track of startX & startY here because that's already been done up above			event.target.startDrag(true);			event.target.parent.addChild(event.target);		}		function stopDragging(event:MouseEvent):void{//, obj:Object) {			//obj.stopDrag();			event.target.stopDrag();			// check to see if the event target is touching target_mc using hitTestObject;			if (event.target.hitTestObject(img_head))			{				event.target.buttonMode = false;				event.target.x = -400;				event.target.y = -400;				// move all circles OTHER than the current target back to their original positions				for each (var circleMC:MovieClip in circles)				{					if (event.target != circleMC)					{						circleMC.x = circleMC.startX;						circleMC.y = circleMC.startY;					}				}			}			else{				// only need to move the event target back if it was dropped outside of target_mc				event.target.x = event.target.startX;				event.target.y = event.target.startY;				event.target.buttonMode = true;			}		}		function showInfo(e:MouseEvent,txtfile:String):void{						fooditem1 = new foodItem(txtfile,"alcodrinks");			fooditem1.getClose().addEventListener(MouseEvent.MOUSE_DOWN, closeInfo);			this.addChild(fooditem1);		}		function getBackButton():Object{			return this.backbutton;		}				function closeInfo(e:MouseEvent):void{			this.removeChild(fooditem1);		}	}}