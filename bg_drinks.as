package  {		import flash.display.MovieClip;	import pq.multitouch.*;			public class bg_drinks extends MovieClip {		var myhot_label;		var mycold_label;		var myalcohol_label;				var myhot;		var mycold;		var myalcohol;		var distance = 75;		var aligntop = 10;				public function bg_drinks() {			// constructor code			this.width = 428;//480;			this.height = 252;//270;						createContents();		}						function createContents(){									myhot = new hotdrinks();			myhot.x = -200;			myhot.y = aligntop;						mycold = new colddrinks();			mycold.y = aligntop - 120;						myalcohol = new alcohol();			myalcohol.x = 200;			myalcohol.y = aligntop;						this.addChild(myhot);			this.addChild(mycold);			this.addChild(myalcohol);						myhot_label = new hot_label();			myhot_label.x = myhot.x;			myhot_label.y = myhot.y + distance;						mycold_label = new cold_label();			mycold_label.y = mycold.y + distance;						myalcohol_label = new alcohol_label();			myalcohol_label.x = myalcohol.x+ 10;			myalcohol_label.y = myalcohol.y + distance;									this.addChild(myhot_label);			this.addChild(mycold_label);			this.addChild(myalcohol_label);		}				function getHotDrinksButton():Object{			return this.myhot;		}		function getColdDrinksButton():Object{			return this.mycold;		}		function getAlcoholButton():Object{			return this.myalcohol;		}	}	}