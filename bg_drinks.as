package  {		import flash.display.MovieClip;	import pq.multitouch.*;			public class bg_drinks extends MovieClip {		var myhot_label;		var mycold_label;		var myalcohol_label;					public function bg_drinks() {			// constructor code			this.width = 428;//480;			this.height = 252;//270;						createContents();		}						function createContents(){			myhot_label = new hot_label();						mycold_label = new cold_label();			myalcohol_label = new alcohol_label();						this.addChild(myhot_label);			this.addChild(mycold_label);			this.addChild(myalcohol_label);		}	}	}